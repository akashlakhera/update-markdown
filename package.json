{
  "name": "update-markdown",
  "version": "1.1.0",
  "description": "Updates part of existing markdown document",
  "main": "index.js",
  "bin": {
    "um": "./bin/update-markdown.js"
  },
  "scripts": {
    "test": "mocha spec/*-spec.js",
    "style": "jscs *.js --fix",
    "example": "DEBUG=um um example/input.md '## foo' example/section.md",
    "cat-example": "cat example/section.md | DEBUG=um um example/input.md '## foo'",
    "3rd-party": "modules-used | um README.md '### 3rd party modules'"
  },
  "pre-commit": [
    "npm run style",
    "npm run example",
    "npm run cat-example",
    "npm run 3rd-party"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/update-markdown.git"
  },
  "keywords": [
    "markdown",
    "md",
    "update",
    "cli"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/update-markdown/issues"
  },
  "homepage": "https://github.com/bahmutov/update-markdown#readme",
  "dependencies": {
    "bluebird": "2.10.1",
    "check-more-types": "2.1.1",
    "debug": "2.2.0",
    "get-stdin-promise": "0.1.1",
    "lazy-ass": "0.6.0",
    "lodash": "3.10.1",
    "marked": "0.3.5",
    "marked-to-md": "1.0.1"
  },
  "devDependencies": {
    "describe-it": "1.4.0",
    "jscs": "2.1.1",
    "mocha": "2.3.3",
    "modules-used": "1.1.0",
    "pre-git": "0.6.2"
  }
}
